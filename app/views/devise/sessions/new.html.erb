<div class="article">

   <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
   <div class="form">
       <h1>Log in</h1>
       <%= f.label :email %><br />
       <%= f.email_field :email, autofocus: true, class: "form_field" %>
   </div>


     <div class="form">
       <%= f.label :password %><br />
       <%= f.password_field :password, autocomplete: "off", class: "form_field" %>
     </div>

     <% if devise_mapping.rememberable? -%>
     <div class="form">
         <%= f.check_box :remember_me %>
         <%= f.label :remember_me %>
       </div>
     <% end -%>

     <div class="form">
       <%= f.submit "Log in", class: "form_button" %>
       <label>
     </div>
   <% end %>
</div>
